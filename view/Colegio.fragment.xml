<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
	<TableSelectDialog id="oTSDColegio" noDataText="No se encontraron colegios" title="Seleccionar Colegio"
		search="handleColegioSearch" confirm="handleColegioConfirm" close="handleColegioClose">
		<ColumnListItem>
			<cells>
				<ObjectIdentifier title="{Stext}"/>
				<Text text="{Objid}"/>
				<Text text="{Landx50}"/>
				<Text text="{Bezei}"/>
				<Text text="{Ort01}"/>
			</cells>
		</ColumnListItem>
		<columns>
			<Column width="40%">
				<header>
					<Text text="Colegio"/>
				</header>
			</Column>
			<Column minScreenWidth="Tablet" demandPopin="true">
				<header>
					<Text text="Codigo"/>
				</header>
			</Column>
			<Column minScreenWidth="Tablet" demandPopin="true" width="20%">
				<header>
					<Text text="Pais"/>
				</header>
			</Column>
			<Column minScreenWidth="Tablet" demandPopin="true" hAlign="Right">
				<header>
					<Text text="Region"/>
				</header>
			</Column>
			<Column minScreenWidth="Tablet" demandPopin="true" hAlign="Center">
				<header>
					<Text text="Ciudad"/>
				</header>
			</Column>
		</columns>
	</TableSelectDialog>
</core:FragmentDefinition>